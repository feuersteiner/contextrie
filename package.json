{
  "name": "contextrie",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "readme.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./session": {
      "types": "./dist/core/session/index.d.ts",
      "import": "./dist/core/session/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "contextrie": "./dist/cli/index.js"
  },
  "scripts": {
    "test": "bun test",
    "dev": "bun --hot ./index.ts",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "link": "bun link",
    "test": "bun test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/azure": "^3.0.18",
    "@ai-sdk/openai-compatible": "^2.0.16",
    "ai": "^6.0.43",
    "human-id": "^4.1.3",
    "zod": "^4.3.6"
  }
}
